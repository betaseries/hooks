{
  "name": "gonetcats/hooks",
  "description": "Command-line tool to interact with hooks.yml files.",
  "type": "library",
  "keywords": [
    "hooks",
    "webhooks"
  ],
  "license": "WTFPL",
  "authors": [
    {
      "name": "Maxime Valette",
      "email": "maxime@maximevalette.com",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/gonetcats/hooks/issues"
  },
  "require": {
    "php": ">=5.4",
    "lib-curl": "*",
    "symfony/console": "~2.1|~3.0",
    "swiftmailer/swiftmailer": "@stable",
    "symfony/yaml": "~2.0|~3.0",
    "sensiolabs/ansi-to-html": "~1.0"
  },
  "require-dev": {
    "sensio/generator-bundle": "~2.3",
    "phpunit/phpunit": "4.2.*",
    "m6web/coke": "dev-master",
    "m6web/symfony2-coding-standard": "dev-master"
  },
  "config": {
    "bin-dir": "bin"
  },
  "bin": [
    "hooks"
  ],
  "autoload": {
    "psr-0": {
      "Hooks": "src/"
    }
  }
}
